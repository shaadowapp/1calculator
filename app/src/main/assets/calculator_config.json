        {
          "id": "algb01lin01",
          "name": "Linear Equations",
          "inputs": [
            { "id": "a", "label": "a", "type": "number" },
            { "id": "b", "label": "b", "type": "number" },
            { "id": "c", "label": "c", "type": "number" }
          ],
          "calculateButton": { "label": "Solve" },
          "result": { "label": "x" },
          "logic": "solveLinearEquation",
          "formula": "ax + b = c\nx = (c - b) / a",
          "example": "a=2, b=3, c=7 → x = (7-3)/2 = 2"
        },
        {
          "id": "algb01qua01",
          "name": "Quadratic Equations",
          "inputs": [
            { "id": "a", "label": "a", "type": "number" },
            { "id": "b", "label": "b", "type": "number" },
            { "id": "c", "label": "c", "type": "number" }
          ],
          "calculateButton": { "label": "Solve" },
          "result": { "label": "x₁, x₂" },
          "logic": "solveQuadraticEquation",
          "formula": "ax² + bx + c = 0\nx = [-b ± sqrt(b²-4ac)] / 2a",
          "example": "a=1, b=-3, c=2 → x₁=2, x₂=1"
        },
        {
          "id": "algb01sys01",
          "name": "System of Equations",
          "inputs": [
            { "id": "a1", "label": "a₁", "type": "number" },
            { "id": "b1", "label": "b₁", "type": "number" },
            { "id": "c1", "label": "c₁", "type": "number" },
            { "id": "a2", "label": "a₂", "type": "number" },
            { "id": "b2", "label": "b₂", "type": "number" },
            { "id": "c2", "label": "c₂", "type": "number" }
          ],
          "calculateButton": { "label": "Solve" },
          "result": { "label": "x, y" },
          "logic": "solveSystemOfEquations",
          "formula": "a₁x + b₁y = c₁\na₂x + b₂y = c₂\nx = (c₁b₂ - c₂b₁)/(a₁b₂ - a₂b₁)\ny = (a₁c₂ - a₂c₁)/(a₁b₂ - a₂b₁)",
          "example": "a₁=2, b₁=3, c₁=8; a₂=1, b₂=-4, c₂=-2 → x=2, y=1"
        },
        {
          "id": "algb01pol01",
